@model List<ASME_CBI>

<div class="container">
    <div class="row pt-4 pb-3 ">
        <div class="col-6">
            <h3 class="text-primary text-center">
                ASME SECTION IV - HLW
            </h3>
            <h3 class="text-primary text-center">
                CONQUEST / BRIGAGDE TANK FABRICATION / INSPECTION REPORT
            </h3>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="ASME" asp-action="Create_CBI_Tank" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> CREATE CBI TANK
            </a>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="ASME" asp-action="Create_CBI_HX" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> CREATE CBI HX
            </a>
        </div>
    </div>
    <table class="table table-bordered table-striped">
        <thead>
        <tr>
            <th style="background-color:black">
                JOB TRAVELER #
            </th>
        </tr>
        </thead>
        <tbody>
            @foreach( var obj in Model.OrderBy(u=>u.JOB_TRAVELER_NUMBER))
            {
                <tr>
                    <td>@obj.JOB_TRAVELER_NUMBER</td>
                </tr>
            }
        </tbody>
    </table>
</div>